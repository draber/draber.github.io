{
  "manifest_version": 2,
  "name": "{{label}}",
  "description": "{{description}}",
  "homepage_url": "{{url}}",
  "version": "{{version}}",
  "content_scripts": [{
    "matches": [
      "{{targetUrl}}*"
    ],
    "js": [
      "content.js"
    ],
    "run_at": "document_start"
  }],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "64": "assets/icon64.png",
    "128": "assets/icon128.png"
  },
  "web_accessible_resources": [
    "{{sbaFileName}}"
  ]
}